{
  "manifest_version": 3,
  "name": "KamaFood",
  "description": "Custom Filter for Snappfood!",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://snappfood.ir/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
